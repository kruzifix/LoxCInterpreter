fun counter(_list)
{
    var res = {};
    for (var i = 0; i < len(_list); i = i + 1)
    {
        var key = _list[i];
        if (!has(res, key))
        {
            set(res, key, 0);
        }

        set(res, key, res[key] + 1);
    }
    return res;
}

print(counter(["a", "b", "c", "a", "c", "d"]));